          ],
          "measurement": "pcap_stats",
          "orderByTime": "ASC",
          "policy": "default",
          "query": "SELECT derivative(max(\"value\"), $__interval) FROM \"pcap_stats\" WHERE (\"server\" =~ /^$server$/ AND \"node\" =~ /^$node$/) AND $timeFilter GROUP BY time($__interval), \"pcap_stat\" fill(none)",
